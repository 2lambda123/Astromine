{
  "schemaVersion": 1,
  "id": "astromine-core",
  "version": "${version}",
  "name": "Astromine: Core",
  "description": "The core dependencies and utilities required for all other Astromine modules.",
  "authors": [
    "vini2003",
    "Shnupbups",
    "Zundrel",
    "shedaniel",
    "MoonlitLamps",
    "SuperCoder79",
    "Draylar",
    "HalfOf2",
    "Martmists",
    "AK9"
  ],
  "contact": {
    "sources": "https://github.com/Mixinors/Astromine",
    "issues": "https://github.com/Mixinors/Astromine/issues",
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/astromine-fabric"
  },
  "license": "MIT",
  "icon": "assets/astromine/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.chainmailstudios.astromine.AstromineCommon"
    ],
    "client": [
      "com.github.chainmailstudios.astromine.AstromineClient"
    ],
    "server": [
      "com.github.chainmailstudios.astromine.AstromineDedicated"
    ],
    "modmenu": [
      "com.github.chainmailstudios.astromine.client.modmenu.AstromineModMenuSupport"
    ],
    "cardinal-components-item": [
      "com.github.chainmailstudios.astromine.registry.AstromineComponents"
    ],
    "cardinal-components-entity": [
      "com.github.chainmailstudios.astromine.registry.AstromineComponents"
    ],
    "cardinal-components-world": [
      "com.github.chainmailstudios.astromine.registry.AstromineComponents"
    ],
    "cardinal-components-chunk": [
      "com.github.chainmailstudios.astromine.registry.AstromineComponents"
    ],
    "cardinal-components-block": [
      "com.github.chainmailstudios.astromine.registry.AstromineComponents"
    ],
    "rei_plugins": [
      "com.github.chainmailstudios.astromine.client.rei.AstromineDiscoveriesRoughlyEnoughItemsPlugin",
      "com.github.chainmailstudios.astromine.client.rei.AstromineFoundationsRoughlyEnoughItemsPlugin",
      "com.github.chainmailstudios.astromine.client.rei.AstromineTechnologiesRoughlyEnoughItemsPlugin"
    ],
    "rei_containers": [
      "com.github.chainmailstudios.astromine.common.rei.AstromineTechnologiesServerContainerPlugin"
    ]
  },
  "mixins": [
    "astromine.mixins.json"
  ],
  "accessWidener": "astromine.accesswidener",
  "depends": {
    "fabricloader": ">=0.9.2",
    "fabric": ">=0.21.0",
    "minecraft": "~1.16.2"
  },
  "custom": {
    "modmenu:parent": "astromine",
    "cardinal-components": [
      "astromine:world_network_component",
      "astromine:chunk_atmosphere_component",
      "astromine:world_holo_bridge_component",
      "astromine:item_inventory_component",
      "astromine:fluid_inventory_component",
      "astromine:energy_inventory_component",
      "astromine:block_entity_transfer_component",
      "astromine:block_entity_redstone_component",
      "astromine:entity_oxygen_component"
    ]
  }
}
