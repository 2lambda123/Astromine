{
  "schemaVersion": 1,
  "id": "astromine-core",
  "version": "${version}",
  "name": "Astromine: Core",
  "description": "The core dependencies and utilities required for all other Astromine modules.",
  "authors": [
    "vini2003",
    "Shnupbups",
    "Zundrel",
    "shedaniel",
    "MoonlitLamps",
    "SuperCoder79",
    "Draylar",
    "HalfOf2",
    "AK9"
  ],
  "contact": {
    "sources": "https://github.com/Chainmail-Studios/Astromine"
  },
  "license": "MIT",
  "icon": "assets/astromine/icon.png",
