  "custom": {
    "cardinal-components": [
      "astromine:network",
      "astromine:atmosphere",
      "astromine:bridge",
      "astromine:item",
      "astromine:fluid",
      "astromine:energy",
      "astromine:transfer",
      "astromine:redstone",
      "astromine:oxygen"
    ]
